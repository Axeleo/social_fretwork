
<div class="musos-list">
    <div class="page-title-container">
        <div class="filler1"><h1 class="h1-muso">Our Musos</h1></div>
        <div class="filler"></div>
        <div class="filler"></div>
    </div>

    <div class="wrap">
        <% @musos.each do |muso| %>
            <a href="/musos/<%= muso.id %>">
                <div class="tile" style="background-image: url(<%= muso.avatar.url(:view) %>);">
                    <div class="text">
                        <h2><%= muso.name %></h2>
                        <p><%= muso.location %></p>
                    </div>
                </div>
            </a>
        <% end %>
    </div>
</div>